run
    num numero = 12;
    sym caracter = 'a';
    dec decimal = 12.00;
    bin booleano  = true;
    set(tucara)
            opt (15)
                dec deci1 = 33.2;
            /opt 
    /set

    bin act funcion() 
        sym algo = 'a';
        con(deci1 == 0)
            bin flag = 1;
        /con
        yet
            bin flag = 0;
        /yet
        ret flag;
    /act

    rep(i=0;i<10;i=i+1)
        numero = numero + i;
    /rep

    til(i<10)
        sym s = 'p';
    /til
/run